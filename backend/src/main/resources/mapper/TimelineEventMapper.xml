<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="org.qingliul.digitalhuangbinhon.mapper.TimelineEventMapper">

    <select id="findAllOrderByYear" resultType="org.qingliul.digitalhuangbinhon.entity.TimelineEvent">
        SELECT * FROM timeline_event ORDER BY year ASC
    </select>

    <select id="findById" resultType="org.qingliul.digitalhuangbinhon.entity.TimelineEvent">
        SELECT * FROM timeline_event WHERE event_id = #{eventId}
    </select>


</mapper>